{
    "opts": {
        "template": "./node_modules/docdash",
        "destination": "doc",
        "recurse": true
    },
    "source": {
        "include" : [ "src" ]
    },
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "docdash" : {
        "static" : true
    }
}